.selectSessionModal
	.session
		border-bottom: solid 1px #ccc
		cursor: pointer
		padding: 1rem

		.badge
			margin-left: 0.5rem
	
	.session.corrupted
		color: red
		cursor: default
	
	.session.corrupted:hover
		background-color: inherit
	
	.session:hover
		background-color: #dddddd
	
	.session.selected
		background-color: #cccccc